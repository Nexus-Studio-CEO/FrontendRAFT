{
  "name": "@nexusstudio/frontendraft",
  "version": "0.1.0",
  "description": "Transform browsers into API servers with RAFT protocol - Based on CSOP",
  "main": "src/index.js",
  "type": "module",
  "exports": {
    ".": "./src/index.js",
    "./react": "./src/plugins/react.jsx",
    "./vue": "./src/plugins/vue.js",
    "./utils": "./src/utils/index.js"
  },
  "scripts": {
    "test": "echo \"Testing with build-and-publish.py\" && python3 scripts/build-and-publish.py --test-only",
    "build": "python3 scripts/build-and-publish.py --build-only",
    "publish:cdn": "python3 scripts/build-and-publish.py --publish",
    "dev": "python3 -m http.server 8000",
    "lint": "echo \"Linting disabled - Pure JS\"",
    "docs": "echo \"Documentation in /docs folder\""
  },
  "keywords": [
    "api",
